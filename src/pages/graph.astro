---
import Layout from "../layouts/Layout.astro";
import { Tree } from "../components/Tree";
---

<Layout title="Linn Strand">
  <main class="w-screen text-center">
    <Tree client:only="react" />
  </main>
</Layout>

<style>
  path {
    fill: none;
  }
  .tree-container {
    display: flex;
    position: relative;
    flex-direction: row;
  }

  .lines {
    fill: none;
    stroke-opacity: 0.6;
    stroke-width: 1;
  }
</style>
